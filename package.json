{
    "name": "pathfinders-monorepo",
    "private": true,
    "scripts": {
        "dev:backend": "npm --prefix BackEnd/server run dev",
        "dev:frontend": "npm --prefix client           run dev",
        "start": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "setup": "npm install @mapbox/polyline @turf/turf && npm install --prefix client && npm install --prefix BackEnd/server && npm install --prefix BackEnd/server node-fetch@2"
    },
    "devDependencies": {
        "concurrently": "^7.6.0"
    },
    "dependencies": {
        "@mapbox/polyline": "^1.2.1",
        "@turf/turf": "^7.2.0",
        "axios": "^1.9.0",
        "dotenv": "^16.5.0"
    },
    "version": "1.0.0",
    "description": "Generate suitable running routes based on user preferences, customized to individual users",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/darrensimmx/PathFinders.git"
    },
