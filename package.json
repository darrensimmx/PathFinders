{
    "name": "pathfinders-monorepo",
    "private": true,
    "scripts": {
        "dev:backend": "npm --prefix BackEnd/server run dev",
        "dev:frontend": "npm --prefix client           run dev",
        "build:frontend": "npm --prefix client run build",
        "build:backend": "echo \"No backend build step\"",
        "build": "npm run build:frontend",
        "start:backend": "npm --prefix BackEnd/server run start",
        "start:frontend": "npm --prefix client run preview -- --port 5173",
        "start": "npx concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "setup": "node scripts/setup.js"
    },
    "devDependencies": {
        "concurrently": "^7.6.0",
        "jest": "^30.0.0",
        "supertest": "^7.1.1"
    },
    "dependencies": {
        "@mapbox/polyline": "^1.2.1",
        "@turf/turf": "^7.2.0",
        "axios": "^1.9.0",
        "bcrypt": "^6.0.0",
        "dotenv": "^16.5.0",
        "html2canvas": "^1.4.1",
        "mongodb": "^6.17.0"
    },
    "version": "1.0.0",
    "description": "Generate suitable running routes based on user preferences, customized to individual users",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/darrensimmx/PathFinders.git"
    },
    "keywords": [],
